# Frontend Staging Environment Configuration

# API Configuration
VITE_API_URL=https://staging-api.yourapp.com
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Monitoring
VITE_SENTRY_DSN=your-staging-sentry-dsn
VITE_ENVIRONMENT=staging
VITE_RELEASE_VERSION=$npm_package_version

# Authentication
VITE_AUTH_DOMAIN=staging-auth.yourapp.com
VITE_AUTH_CLIENT_ID=your-staging-auth-client-id

# External Services
VITE_STRIPE_PUBLIC_KEY=your-staging-stripe-public-key

# Performance
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLE_RATE=1.0

# Logging
VITE_LOG_LEVEL=info
VITE_CONSOLE_LOGGING=true
